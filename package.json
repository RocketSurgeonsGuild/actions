{
  "name": "rsg-actions",
  "version": "0.0.0",
  "private": true,
  "description": "Rocket Surgeons Actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint --fix **/*.ts",
    "dist": "npm run dist:merge-labels && npm run dist:sync-milestone && npm run dist:auto-label",
    "dist:merge-labels": "ncc build ./merge-labels/src/main.ts -o ./merge-labels/dist/",
    "dist:sync-milestone": "ncc build ./sync-milestone/src/main.ts -o ./sync-milestone/dist/",
    "dist:auto-label": "ncc build ./auto-label/src/main.ts -o ./auto-label/dist/",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run dist && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Rocket Surgeons Guild",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "1.1.0",
    "@actions/cache": "3.0.6",
    "@actions/core": "1.9.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.0.3",
    "@actions/glob": "0.3.0",
    "@actions/http-client": "2.0.1",
    "@actions/io": "1.1.2",
    "@actions/tool-cache": "2.0.1",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "semver": "7.3.8"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/js-yaml": "4.0.5",
    "@types/lodash-es": "4.17.6",
    "@types/node": "17.0.24",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-rule-documentation": "1.0.23",
    "husky": "8.0.2",
    "ix": "4.6.1",
    "jest": "28.1.3",
    "jest-circus": "28.1.3",
    "jest-cli": "28.1.3",
    "js-yaml": "4.1.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rxjs": "7.5.7",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
